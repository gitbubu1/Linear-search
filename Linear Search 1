void main()
{
    int a[100],i,search,k=0;

    printf("Enter 10 numbers: \n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter a number you want to search");
    scanf("%d",&search);

    for(i=0;i<100;i++)
    {
        if(a[i]==search)
        {
            printf(" Number is found");
            k++;
            break;
        }
    }
        if (k==0)
        {
            printf("The number is not found");

        }
    getch();

}
